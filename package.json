{
  "name": "vite_react_shadcn_ts",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "predev": "npm run stop:ports",
    "dev": "vite",
    "build": "vite build",
    "build:dev": "vite build --mode development",
    "lint": "eslint .",
    "preview": "vite preview",
    "stop:ports": "bash -lc 'for p in 8080 8081 5173 3000; do pid=$(lsof -t -i:$p); if [ -n \"$pid\" ]; then kill -9 $pid || true; fi; done'",
    "stop:range": "bash -lc 'for p in $(seq 8080 8090); do pid=$(lsof -t -i:$p); if [ -n \"$pid\" ]; then kill -9 $pid || true; fi; done'",
    "test": "vitest run",
    "test:unit": "vitest run tests/unit",
    "test:integration": "vitest run tests/integration",
    "test:watch": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest run --coverage",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:headed": "playwright test --headed",
    "test:e2e:debug": "playwright test --debug",
    "test:e2e:critical": "playwright test tests/e2e/meal-day-assignment-race-condition.spec.ts",
    "test:e2e:ai": "vitest run --config vitest.e2e-ai.config.ts",
    "test:e2e:ai:watch": "vitest --config vitest.e2e-ai.config.ts",
    "test:e2e:ai:breakfast": "vitest run tests/e2e-ai/scenarios/breakfast-optimization.e2e.test.ts --config vitest.e2e-ai.config.ts",
    "test:e2e:ai:lunch": "vitest run tests/e2e-ai/scenarios/lunch-optimization.e2e.test.ts --config vitest.e2e-ai.config.ts",
    "test:e2e:ai:dinner": "vitest run tests/e2e-ai/scenarios/dinner-optimization.e2e.test.ts --config vitest.e2e-ai.config.ts",
    "test:e2e:ai:edge": "vitest run tests/e2e-ai/scenarios/edge-cases.e2e.test.ts --config vitest.e2e-ai.config.ts",
    "test:all": "npm run test && npm run test:e2e"
  },
